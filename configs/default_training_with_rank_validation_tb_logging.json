{
	"dataset_reader": {
            "type":
            "openke-dataset",
            "dataset_name":
            "FB15K237",
            "all_datadir":
            "/Users/dhruv/UnsyncedDocuments/IESL/kb_completion/datasets/.data/test",
            "mode":
            "train",
            "number_negative_samples":
            1
        },
	"validation_dataset_reader":{
	    "type":
            "openke-rank-validation-dataset",
            "dataset_name":
            "FB15K237",
            "all_datadir":
            "/Users/dhruv/UnsyncedDocuments/IESL/kb_completion/datasets/.data/test"
            },
	"train_data_path": "dummpy_path",
	"validation_data_path": "dummy_path",
	"datasets_for_vocab_creation": [],
	"iterator":{
		"type":"basic",
		"batch_size": 1
	},
	"validation_iterator":{
		"type":"single-sample-rank-validation-iterator",
		"batch_size":1
	},
        "model": {
            "type": "max-margin-conditional-box-model",
            "num_entities": 7,
            "num_relations": 5,
            "embedding_dim": 3,
            "box_type": "DeltaBoxTensor",
	    "margin":50,
	    "debug": false
        },
	"trainer":{
		"type":"callback",
		"callbacks":[
			{
				"type": "debug-validate",
                                "debug": true,
				"log_freq": 2
			},
			{
				"type": "checkpoint",
				"checkpointer":{
					"num_serialized_models_to_keep":1
				}
			},
			{
				"type": "track_metrics",
				"patience": 3,
				"validation_metric": "-avg_rank"
			},
			{
				"type": "tensorboard_logging",
				"log_freq":2,
				"debug": true
			},
			{
				"type": "log_metrics_to_wandb",
				"debug": true,
				"debug_log_freq": 2
			}
		],
		"optimizer": {
			"type": "sparse_adam",
			"lr":0.01
		},
		"cuda_device": -1,
		"num_epochs": 100,
		"shuffle": true
	}

}
