        },
	"trainer":{
		"type":"callback",
		"callbacks":[
			{
				"type": "debug-validate",
				"debug": true
			},
			{
				"type": "checkpoint",
				"checkpointer":{
					"num_serialized_models_to_keep":1
				}
			},
			{
				"type": "track_metrics",
				"patience": 3,
				"validation_metric": "-avg_rank"
			},
                        {
				"type": "log_metrics_to_wandb",
				"debug": true
			}
		],
		"optimizer": {
			"type": "sparse_adam",
			"lr":0.01
		},
		"cuda_device": 0,
		"num_epochs": 25,
		"shuffle": true
	}

